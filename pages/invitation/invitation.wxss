page {
    height: 100%;
    justify-content: center;
}

.musicIconCotainer {
    /* border: 2rpx solid black; */
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    top: 30rpx;
    right: 30rpx;
    z-index: 9999;
  }

.imgContainer {
    /* border: 2rpx solid black; */
    width: 50%;
    height: 500rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    opacity: 0;
}

.imgContainer-image {
    border: 20rpx solid white;
    border-radius: 20rpx;
    /* box-shadow: 5rpx 5rpx 15rpx 5rpx #666666 */
}

.textContainer {
    /* border: 2rpx solid black; */
    margin-top: 30rpx;
    width: 70%;
    height: 250rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    opacity: 0;
}

.brace {
    /* border: 2rpx solid black; */
    width: 10%;
}

.text {
    /* border: 2rpx solid black; */
    width: 40%;
}

.timeAndAdress {
    /* border: 2rpx solid black; */
    margin-top: 30rpx;
    width: 90%;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: white;
    opacity: 0;
}

.adress {
    margin-top: 10rpx;
}

.button {
    border: 2rpx solid white;
    color:white;
    margin-top: 50rpx;
    border-radius: 50rpx;
    width: 30%;
    height: 70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    opacity: 0;
}